<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mini_proect">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mini_proect">
        <activity
            android:name=".Activities.Home_screen_employee"
            android:exported="true" />
        <activity
            android:name=".Activities.ChangePassword"
            android:exported="true" />
        <activity
            android:name=".save_data"
            android:exported="true" />
        <activity
            android:name=".Activities.Register"
            android:exported="true"
            android:label="Registration_form" />
        <activity
            android:name=".Activities.Home_screen_admin"
            android:exported="true"
            android:label="ADMIN" />
        <activity
            android:name=".Activities.Home_screen"
            android:exported="true"
            android:label="Home_Screen" />
        <activity
            android:name=".Activities.login"
            android:exported="true"
            android:label="Login" />
        <activity
            android:name=".Activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>